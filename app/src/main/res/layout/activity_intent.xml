<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.IntentActivity">

    <Button
        android:id="@+id/start_show_icon_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="START_SHOW_ICON_APP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.010"
        tools:ignore="MissingConstraints" />
    <Button
        android:id="@+id/start_hide_icon_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="START_HIDE_ICON_APP"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_show_icon_app"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="MissingConstraints"/>
    <Button
        android:id="@+id/start_ydhl_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="START_YDHL_APP"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_hide_icon_app"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="MissingConstraints"/>
    <Button
        android:id="@+id/start_wifi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="START_WIFI"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_ydhl_app"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="MissingConstraints"/>
    <Button
        android:id="@+id/start_calculator"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="START_calculator"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_wifi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="MissingConstraints"/>
    <Button
        android:id="@+id/start_xicoo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="跳转第三方新科APP"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_calculator"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="MissingConstraints"/>
    <TextView
        android:id="@+id/start_xicoo_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_xicoo"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="MissingConstraints"/>

</androidx.constraintlayout.widget.ConstraintLayout>