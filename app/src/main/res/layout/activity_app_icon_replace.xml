<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.AppIconReplaceActivity">

    <Button
        android:id="@+id/set_app_icon_default"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="set_app_icon_default"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0"/>
    <Button
        android:id="@+id/set_app_icon_alias2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="set_app_icon_alias2"
        app:layout_constraintTop_toBottomOf="@+id/set_app_icon_default"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.001"/>

</androidx.constraintlayout.widget.ConstraintLayout>